  hs-source-dirs: bench
  main-is: Semaphore.hs
  type: exitcode-stdio-1.0
  ghc-options: -Wall -O -rtsopts -threaded "-with-rtsopts=-N4 -qg"
  build-depends:
      mitchell-stdlib
  default-language: Haskell2010
